@model IEnumerable<EasyMission.Domaine.reclamation>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- body-content -->
<div class="body-content clearfix">

    <div class="bg-color1">
        <div class="container">

            <div class="col-md-9 col-sm-9">
                <div class="block-section box-side-account">
                    <h3 class="no-margin-top">Reclamation</h3>
                    <hr />
                    <div class="table-responsive">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>Date Reclamation</th>
                                    <th>User</th>
                                    <th>Title</th>

                                   
                                </tr>
                            </thead>
                            <tbody>
@foreach (var item in Model)
{
                                <tr>
                                    <th scope="row">@Html.DisplayFor(modelItem => item.idReclamtion)</th>
                                    <td>@Html.DisplayFor(modelItem => item.dateSubmit)</td>
                                    <td>@Html.DisplayFor(modelItem => item.aspnetuser1.Name)</td>
                                    <td>@Html.DisplayFor(modelItem => item.aspnetuser1.cvs.First().title)</td>
                                

                                    </tr>

}
                            </tbody>
                        </table>
                    </div>

                    <!-- pagination -->
                    <nav>
                        <ul class="pagination pagination-theme  ">
                            <li>
                                <a href="#" aria-label="Previous">
                                    <span aria-hidden="true">&larr;</span>
                                </a>
                            </li>
                            <li class="active"><a href="#">1</a></li>
                            <li><a href="#">2</a></li>
                            <li><a href="#">3</a></li>
                            <li>
                                <a href="#" aria-label="Next">
                                    <span aria-hidden="true">&rarr;</span>
                                </a>
                            </li>
                        </ul>
                    </nav><!-- pagination -->

                </div>
            </div>
        </div>
    </div>
    <!-- modal Advanced search -->
  

</div><!--end body-content -->


