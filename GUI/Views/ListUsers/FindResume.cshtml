                                        <h5><span class="color-black">@Html.DisplayFor(modelItem => item.cvs.First().location)</span></h5>
                                        <p>
                                          @Html.DisplayFor(modelItem => item.cvs.First().description)
                                        </p>
                                       
                                    </div>
                                </div>
                            </div><!-- end item listing  -->
}
