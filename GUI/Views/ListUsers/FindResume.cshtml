@model IEnumerable<EasyMission.Domaine.aspnetuser>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
 <!-- body-content -->
<div class="body-content clearfix">

    <div class="bg-color2">
        <div class="container">
